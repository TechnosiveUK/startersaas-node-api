{
  "name": "startersaas-node",
  "version": "1.0.0",
  "description": "Starter SAAS Node",
  "main": "index.js",
  "scripts": {
    "start": "cd build && node main",
    "dev": "nodemon build.dev.js | pino-pretty ",
    "prod": "node build.prod.js | pino-pretty ",
    "debug": "ndb build.dev.js | pino-pretty",

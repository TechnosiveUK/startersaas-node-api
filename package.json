    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "json2xml": "^0.1.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moneysafe": "^2.2.1",
    "mongoose": "^5.12.10",
    "morgan": "^1.10.0",
    "node-polyglot": "^2.4.0",
